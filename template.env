# Live
REACT_APP_FIREBASE_LIVE_API_KEY=AIxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
REACT_APP_FIREBASE_LIVE_AUTH_DOMAIN=appname.firebaseapp.com
REACT_APP_FIREBASE_LIVE_PROJECT_ID=appname
