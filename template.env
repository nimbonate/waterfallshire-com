REACT_APP_FIREBASE_LIVE_MEASUREMENT_ID=G-xxxxxxxxx
REACT_APP_FIREBASE_LIVE_VAPID_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Test 
REACT_APP_FIREBASE_TEST_API_KEY=AIxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
REACT_APP_FIREBASE_TEST_AUTH_DOMAIN=appname.firebaseapp.com
REACT_APP_FIREBASE_TEST_PROJECT_ID=appname
