REACT_APP_FIREBASE_TEST_MESSAGING_SENDER_ID=xxxxxxxxxxxx
REACT_APP_FIREBASE_TEST_APP_ID=1:xxxxxxxxxxxx:web:xxxxxxxxxxxxxxxxx
REACT_APP_FIREBASE_TEST_MEASUREMENT_ID=G-xxxxxxxxx
REACT_APP_FIREBASE_TEST_VAPID_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Ensure localhost is refresh on save
FAST_REFRESH=false

